def TsDataProcessor(Prices, *other, t=10):
    import pandas as pd
    import numpy as np

    frames = list([Prices])

    for i in other:
        frames.append(i)

    daterange = pd.DataFrame([])

    print("identifying overlap")

    for i in range(len(frames) - 1, -1, -1):
        if type(frames[i]) != pd.Series and type(frames[i]) != pd.DataFrame:
            print ("Only data frames and series may be submitted as arguments except for t, not: " + str(type(frames[i])))
            if type(frames[i]) == pd.Series:
                frames[i] = frames[i].to_frame()
        else:
            daterange = daterange.append(pd.DataFrame([[frames[i].index[0], frames[i].index[-1]]]))

    start = np.datetime64(daterange[0].max())
    end = np.datetime64(daterange[1].min())

    print("converting indices to datetime format and creating dataset template")

    for i in range(0, len(frames)):
        frames[i].index = pd.to_datetime(frames[i].index)

    dataset = frames[0]

    for i in range(1, len(frames)):
        for col in frames[i].columns.values:
            dataset[col] = frames[i][col]
            #dataset[col].add(frames[i][col])

    dataset.dropna(inplace=True)

    #check for NaN in date range and report to avoid tainting data quality


    print("populating day of the week")

    # for i in range(0, 5):
    #     dataset["day" + str(i)] = 0
    # for i in range(0, len(dataset)):
    #     dataset["day" + str(dataset.index[i].dayofweek)].iloc[i] = 1

    return dataset, frames[i][col]
